{
  "name": "csv-kit",
  "version": "0.1.3",
  "description": "Isomorphic CSV parser and generator. Zero dependencies.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "src",
    "!src/**/*.test.ts"
  ],
  "scripts": {
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "publish:jsr": "deno publish"
  },
  "keywords": [
    "csv",
    "parse",
    "generate",
    "isomorphic",
    "typescript",
    "zero-dependencies",
    "rfc4180"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juaquito/csv-kit.git"
  },
  "bugs": {
    "url": "https://github.com/juaquito/csv-kit/issues"
  },
  "homepage": "https://github.com/juaquito/csv-kit#readme",
  "devDependencies": {
    "@types/bun": "^1.3.9"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
